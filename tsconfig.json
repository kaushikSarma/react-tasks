{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": false,
        "module": "commonjs",
        "target": "es6",
        "lib": ["es2015", "es2017", "dom"],
        "jsx": "react",
        "removeComments": true,
        "allowSyntheticDefaultImports": false,
        "moduleResolution": "node",
        "baseUrl": "./src",
        "paths": {
            "@component/*": ["components/*"],
            "@data/*": ["data/*"],
            "@reducer/*": ["reducers/*"],
        },
    },
}